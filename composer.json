{
    "name": "javaabu/schema",
    "description": "Extends the Laravel database schema with convenience methods used by javaabu/generators",
    "homepage": "https://github.com/Javaabu/schema",
    "license": "MIT",
    "keywords": [
        "javaabu",
        "laravel",
        "schema"
    ],
    "authors": [
        {
            "name": "Arushad Ahmed",
            "email": "arushad@javaabu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.14 || ^10.40 || ^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.5 || ^10.5",
        "doctrine/dbal": "^2.0 || ^3.0 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\Schema\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\Schema\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\Schema\\SchemaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
